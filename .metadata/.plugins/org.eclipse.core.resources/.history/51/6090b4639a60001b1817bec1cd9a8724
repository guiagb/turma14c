package decisao;

import javax.swing.JOptionPane;

public class DecisaoSimples {

	public static void main(String[] args) {
		String disciplina = JOptionPane.showInputDialog("Insira a disciplina");
		double nota1 = Double.parseDouble(JOptionPane.showInputDialog("Insira a sua nota 1"));
		double nota2 = Double.parseDouble(JOptionPane.showInputDialog("Insira a sua nota 2"));
		double media = (nota1 + nota2)/2;
		System.out.println("Sua media eh: " + media);

	}

}
